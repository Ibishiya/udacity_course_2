import pandas as pd

df = pd.read_csv('/content/sample_data/california_housing_train.csv')

display(df.head())
display(df.info())
display(df.describe())

display(df.isnull().sum())

import matplotlib.pyplot as plt
import seaborn as sns

df.hist(figsize=(10, 8))
plt.tight_layout()
plt.show()

